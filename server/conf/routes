# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                             	controllers.Application.index
GET			/map							controllers.Application.map
GET			/events							controllers.Application.events
GET			/pet							controllers.Application.pet
GET			/settings						controllers.Application.settings
GET			/logout							controllers.Application.logout
GET         /login                     		controllers.Application.login
GET         /accountCreation                controllers.Application.accountCreation
GET         /changePW              			controllers.Application.changePW
GET         /chooseYourPet                  controllers.Application.chooseYourPet
GET			/shop							controllers.Application.shop

# Prefix must match `play.assets.urlPrefix`
GET        /assets/*file                 controllers.Assets.at(file)
GET        /versionedAssets/*file        controllers.Assets.versioned(path="/public", file: Asset)
